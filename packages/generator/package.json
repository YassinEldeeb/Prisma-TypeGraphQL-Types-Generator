{
  "name": "prisma-typegraphql-types-generator",
  "description": "Prisma generator for generating TypeGraphQL class types and enums, It works like a copilot where the generated output is very human readable and can be edited",
  "version": "1.4.23",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-typegraphql-types-generator": "dist/bin.js"
  },
  "engines": {
    "node": ">=12.0"
  },
  "scripts": {
    "build": "tsc",
    "build-w": "tsc -w",
    "prepack": "yarn build",
    "commit": "git-cz",
    "test": "jest",
    "publish-locally": "yalc push"
  },
  "dependencies": {
    "@prisma/client": "^3.6.0",
    "@prisma/generator-helper": "^3.6.0",
    "@prisma/sdk": "^3.6.0",
    "prettier": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.11",
    "@types/prettier": "^2.4.2",
    "jest": "^27.4.3",
    "prisma": "^3.6.0",
    "semantic-release": "^18.0.1",
    "typescript": "^4.5.2"
  },
  "homepage": "https://github.com/YassinEldeeb/Prisma-TypeGraphQL-Types-Generator",
  "repository": "git://github.com/YassinEldeeb/Prisma-TypeGraphQL-Types-Generator.git",
  "author": "Yassin Eldeeb <yassineldeeb94@gmail.com>",
  "keywords": [
    "prisma",
    "prisma2",
    "type-graphql",
    "typegraphql",
    "prisma-framework",
    "graphql",
    "generator",
    "copilot",
    "classes",
    "enums",
    "types"
  ],
  "release": {
    "branches": [
      "main"
    ]
  }
}
